<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
        @(!string.IsNullOrEmpty(ViewData["Title"]?.ToString()) ? ViewData["Title"] + " | Silicon" : "Silicon")
    </title>




    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />


</head>
<body>

    <div class="wrapper">
        <partial name="~/Views/Partials/Sections/_Header.cshtml" />

        <section class="account">
            <div class="container">
                <aside>
                    <partial name="~/Views/Account/_AccountProfile.cshtml" />

                    <nav class="account-nav">
                        <a class="btn-theme btn-small" asp-action="Details" asp-controller="Account"><i class="fa-regular fa-cog"></i>Account Details</a>
                        <a class="btn-theme-secondary btn-small" asp-action="SignOut" asp-controller="Auth"><i class="fa-regular fa-arrow-right-from-bracket"></i>Sign Out</a>
                    </nav>
                </aside>
                <main>
                    @RenderBody()
                </main>
            </div>
        </section>

        <partial name="~/Views/Partials/Sections/_Footer.cshtml" />
    </div>





    <script src="https://kit.fontawesome.com/a041cc045c.js" asp-append-version="true" crossorigin="anonymous"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
